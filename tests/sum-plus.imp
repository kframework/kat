// Looping
// -------

// ### `sum-plus.imp`

// This version of the `sum` proogram has additional statements after the main
// while loop.


int n , s ;

n = 10 ;
while (0 <= n) {
  n = n + -1 ;
  s = s + n ;
}

s = s + 300 ;

