// Looping
// -------

// ### `krazy-loop.imp`


int i , j , k , l , m , s ;

i = 11 ;
j = 11 ;
k = 0 ;
l = 0 ;
m = 0 ;
s = 0 ;

while (0 <= i) {
  k = 0 ;
  j = 11 ;
  while (0 <= j) {
    l = (3 * j) - (j + i) ;
    m = (5 * i) + 1 ;
    if (i <= ((i / 2) * 2)) {
      k = k + (l / i) ;
    } else {
      k = k + (j / m) ;
    }
    j = j - 1 ;
  }
  s = s + ((i * k) / 3) ;
  i = i - 1 ;
}

